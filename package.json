{
  "name": "lorcanalocalmanager",
  "version": "1.0.0",
  "description": "Local manager for Disney Lorcana trading card collections",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm install",
    "test": "jest",
    "lint": "eslint src/",
    "docker:build": "docker build -t lorcanalocalmanager .",
    "docker:run": "docker run -p 8080:8080 lorcanalocalmanager",
    "docker:compose": "docker-compose up --build"
  },
  "keywords": [
    "lorcana",
    "trading-cards",
    "collection-manager",
    "docker",
    "web-app"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5",
    "fs-extra": "^11.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}